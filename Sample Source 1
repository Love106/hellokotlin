fun main (args: Array<String>) {
    var x = 3
    val name = "cormoran"
    x = x * 10 
    print("x is $x.")
    while (x > 20) {
        x = x - 1 
        print("x is now $x")
    }
    for (i in 1..10){
        x = x + 1 
        print("x is now $x")
    }
    if (x == 20){
        print("x must be 20.")    
    } else {
        println("x isn't 20.")
    }
    if (name.equals("cormoran")){
        println("$name Strike")
    }
}
